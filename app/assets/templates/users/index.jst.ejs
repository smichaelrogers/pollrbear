<div class="container-fluid">
  <div class="row">
    <div class="col-xs-12 col-md-6 col-lg-4" id="users">
      <div class="panel panel-default">
        <ul class="list-group">
          <% users.forEach(function(user) { %>
            <a href="#" data-id="<%= user.id %>" class="list-group-item show-user-page">
              <h4 class="list-group-item-heading"><%= user.fullName() %></h4>
              <h6 class="list-group-item-text"><%= user.escape('email') %></h6>
            </a>
          <% }); %>
        </ul>
      </div>
    </div>
  </div>
</div>
