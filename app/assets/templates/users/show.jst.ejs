<div class="col-xs-10 col-xs-offset-1 col-md-7 col-md-offset-0 col-lg-8 col-lg-offset-0" id="left-side">
    <div class="row" id="top-left">
        <div class="col-xs-8">

        </div>
        <div class="col-xs-4" id="user-info">
            <span class="user-wrap">
                <%= user.fullName() %> &nbsp;
                    <small>
                        <%= user.escape('email') %>
                    </small>
            </span>
            <span class="sign-out-button-wrap">
                <button type="button" class="btn btn-std sign-out-link">Sign out</button>
            </span>
        </div>
    </div>
    <div id="logo" class="row">
        <div class="col-xs-6">
            <a href="#" class="nav-logo-wrap reload-main">
                <img class="nav-logo-img" src="/logo13.png">
            </a>
        </div>
        <div class="col-xs-6">

        </div>
    </div>
    <div id="main" class="row">
    </div>
</div>
<div class="col-xs-10 col-xs-offset-1 col-md-5 col-lg-4 col-md-offset-0 col-lg-offset-0 group" id="right-side">
    <div class="row">
        <div class="col-xs-12" id="trending">
            <div class="row">
                <div class="col-xs-12">
                    <h2 class="page-title">Trending</h2>
                    <div id="trending-polls">
                    </div>
                </div>
            </div>
        </div>
        <div class="col-xs-12" id="new-poll">
        </div>
    </div>
</div>
