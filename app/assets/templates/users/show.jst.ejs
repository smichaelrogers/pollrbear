<div id="user-info" class="col-xs-12">
    <div class="row">
        <div class="col-xs-4 col-xs-offset-8">
            <div class="row">
                <div class="col-xs-8">
                    <span>
                        <%= user.fullName() %>
                    </span>
                    <small>
                        <%= user.escape('email') %>
                    </small>
                </div>
                <div class="col-xs-4">
                    <button type="button" class="btn btn-std sign-out-link">Sign out</button>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="col-xs-12" id="inner-wrapper">
    <div class="row">
        <div class="col-xs-10 col-xs-offset-1 col-md-7 col-md-offset-0 col-lg-8 col-lg-offset-0" id="left-side">
            <div class="row" id="main-logo">
                <div class="col-xs-12">
                    <a href="#" class="nav-logo-wrap reload-main">
                        <img class="nav-logo-img" src="/slice52x.png">
                    </a>
                </div>
            </div>
            <div id="main" class="row">
            </div>
        </div>
        <div class="col-xs-10 col-xs-offset-1 col-md-5 col-lg-4 col-md-offset-0 col-lg-offset-0 group" id="right-side">
            <div id="trending">
                <h3>Trending
                    <small>last 24 hours</small>
                </h3>
                <div id="trending-polls">
                </div>
            </div>
            <div class="col-xs-12" id="new-poll">
            </div>
        </div>
    </div>
</div>
