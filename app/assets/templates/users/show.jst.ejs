<div class="row">
  <div class="col-xs-12 col-md-4 col-lg-4">
    <div class="row">
      <div class="col-xs-12" id="new-poll">
      </div>
      <div class="col-xs-12 left-anchor">
      </div>
    </div>
  </div>
  <div class="col-xs-12 col-md-8 col-lg-8">
    <div class="row">
      <div class="col-xs-12 col-md-12 col-lg-8">
        <div class="panel panel-default" id="main">
          <div class="panel-heading">
            <h3 class="panel-title">
              <%= user.fullName() %>'s Active Polls
            </h3>
          </div>
        </div>
      </div>
      <div class="col-xs-6 col-md-6 col-lg-4 right-anchor">
        <div class="panel panel-default" id="user-content">
          <div class="panel-heading">
            <h3 class="panel-title">Trending</h3>
          </div>
          <div class="panel-body">
          </div>
          <div class="panel-footer">
          </div>
        </div>
        <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title"><%= user.escape('first_name') %></h3>
          </div>
          <div class="panel-body">
            <a href="#" class="thumbnail">
              <img class="profile-img img-rounded img-responsive" src="<%= user.get('profile_img') %>">
            </a>
          </div>
          <div class="panel-body">
            <h4>
              <%= user.fullName() %>
            </h4>
            <h5>
              <%= user.escape('email') %>
            </h5>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
