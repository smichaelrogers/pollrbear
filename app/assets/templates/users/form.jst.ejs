<form class="login-form">
  <div class="form-group">
    <label>First Name</label>
    <input class="form-control" name="user[first_name]" type="text" placeholder="First">
  </div>
  <div class="form-group">
    <label>Last Name</label>
    <input class="form-control" name="user[last_name]" type="text" placeholder="Last">
  </div>
  <div class="form-group">
    <label>Email</label>
    <input class="form-control" name="user[email]" type="text" placeholder="Email">
  </div>
  <div class="form-group">
    <label>Password</label>
    <input class="form-control" name="user[password]" type="password" placeholder="Password">
  </div>
  <div class="form-group">
    <button type="submit" class="btn btn-warning pull-right"><i class="fa fa-sign-in fa-fw"></i> Submit</button>
  </div>
  <div class="form-group">
    <a href="/auth/twitter" class="btn btn-default"><i class="fa fa-twitter fa-fw"></i>Twitter</a>
    <a href="#" class="btn btn-default"><i class="fa fa-github fa-fw"></i> Github</a>
  </div>
</form>
