<section id="splash" class="row">
    <div class="col-xs-10 xs-offset-1 col-md-6 col-md-offset-3 login-panel">
        <div class="row">
            <div class="col-xs-12">
                <div class="splash-logo-wrap">
                    <img class="splash-logo" src="/logo13.png">
                </div>
            </div>
            <div class="col-xs-12">
                <div class="register-heading"><h3 class="page-title">Login</h3>
                </div>
            </div>
            <div class="col-xs-12">
                <div class="notice notice-login"> </div>
            </div>
            <div class="col-xs-12">
                <form class="login-form">
                    <div class="form-wrap">
                        <label>Email</label>
                        <input class="form-class form-control" name="user[email]" type="text" placeholder="Email">
                    </div>
                    <div class="form-wrap">
                        <label>Password</label>
                        <input class="form-class form-control" name="user[password]" type="password" placeholder="Password">
                    </div>
                    <div class="button-wrap">
                        <button type="submit" class="btn btn-std btn-signin pull-right" id="standard-login">Submit</button>
                        
                        <button type="button" class="btn btn-std" id="guest-login">Guest Login</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>
<section id="signup" class="row">
    <div class="col-xs-10 xs-offset-1 col-md-6 col-md-offset-3 login-panel">
        <div class="row">
            <div class="col-xs-12">
                <div class="register-heading"><h3 class="page-title">Register</h3>
                </div>
            </div>
            <div class="col-xs-12">
                <div class="notice notice-register"> </div>
            </div>
            <div class="col-xs-12">
                <form class="signup-form">
                    <div class="form-wrap">
                        <label>First Name</label>
                        <input class="form-class form-control" name="user[first_name]" type="text" placeholder="First">
                    </div>
                    <div class="form-wrap">
                        <label>Last Name</label>
                        <input class="form-class form-control" name="user[last_name]" type="text" placeholder="Last">
                    </div>
                    <div class="form-wrap">
                        <label>Email</label>
                        <input class="form-class form-control" name="user[email]" type="text" placeholder="Email">
                    </div>
                    <div class="form-wrap">
                        <label>Password</label>
                        <input class="form-class form-control" name="user[password]" type="password" placeholder="Password">
                    </div>
                    <div class="button-wrap">
                        <button type="submit" class="btn btn-std btn-signin" id="register-login">Submit</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>
