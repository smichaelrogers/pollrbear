<li class="list-group-item">
  <% questions.forEach(function(question) { %>
    <form data-poll-id="<%= pollId %>">
      <% question.answers().forEach(function(answer) { %>
        <div class="radio">
          <label>
            <input type="radio" name="response[answer_id]" value="<%= answer.id %>">
            <%= answer.escape('text') %>
          </label>
        </div>
        <% }); %>
    </form>
    <div class="result-content" data-question-id="<%= question.id %>">
      <a href="#" class="show-result" data-question-id="<%= question.id %>">Result</a>
      </p>
    </div>
  <% }); %>
</li>
<li class="list-group-item">
  <button type="button" class="visitor-submit-form" data-poll-id="<%= pollId %>">Submit</button>
</li>
