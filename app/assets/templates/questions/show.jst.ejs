<ul class="list-group question-data">
  <li class="list-group-item">
    <p class="list-group-item-text">
      <a href="#" class="back-one-level">Back</a>
    </p>
  </li>
  <li class="list-group-item">
    <h3 class="list-group-item-heading">
      <%= question.escape("text") %>
    </h3>
    <p class="list-group-item-text">asdf
    </p>
  </li>
  <% var i = 0; %>
  <% answers.forEach(function(answer) { %>
    <li class="list-group-item">
      <span class="label label-default"><%= percentages[i] %></span>
      <%= answer.text %>
      <% i++ %>
    </li>
    <% }); %>
</ul>
